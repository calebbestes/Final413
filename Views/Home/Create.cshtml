@model Final.Models.Entertainer


@{
    Layout = "_Layout";
}

<h1>Add/Edit a Contact</h1>

<form method="post">
    <input type="hidden" asp-for="EntertainerId"/>
    <select asp-for="EntertainerId">
    </select>
    <br/>
    <label asp-for="EntStageName">Name</label>
    <input asp-for="EntStageName" placeholder="Enter Name"/>
    <br/>
    <label asp-for="EntSsn">Email</label>
    <input asp-for="EntSsn" placeholder="Enter SSN"/>
    <br/>
    <label asp-for="EntStreetAddress">Address</label>
    <input asp-for="EntStreetAddress"/>
    <br/>
    <label asp-for="EntCity">City</label>
    <input asp-for="EntCity"/>
    <br/>
    <label asp-for="EntState">State</label>
    <input asp-for="EntState"/>
    <br/>
    <label asp-for="EntZipCode">Zip</label>
    <input asp-for="EntZipCode"/>
    <br/>
    <label asp-for="EntPhoneNumber">Phone Number</label>
    <input asp-for="EntPhoneNumber"/>
    <br/>
    <label asp-for="EntWebPage">Web Page</label>
    <input asp-for="EntWebPage"/>
    <br/>
    <label asp-for="EntEmailAddress">Email</label>
    <input asp-for="EntEmailAddress" placeholder="Enter Number"/>
    <button type="submit">Submit</button>
</form>